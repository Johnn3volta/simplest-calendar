{
  "name": "simplest-calendar",
  "version": "1.0.0",
  "description": "Simple Calendar Pure JS",
  "main": "dist/simplest-calendar.cjs.js",
  "module": "simplest-calendar.esm.js",
  "browser": "simplest-calendar.umd.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "dev": "rollup -c --w --environment dev",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "npm run lint -- --fix",
    "test": "node tests/test.js",
    "pretest": "npm run build"
  },
  "lint-staged": {
    "./src/**/*.ts": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Johnn3volta/simplest-calendar.git"
  },
  "keywords": [
    "simple",
    "calendar",
    "pure",
    "js"
  ],
  "author": "Johnn3volta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Johnn3volta/simplest-calendar/issues"
  },
  "homepage": "https://github.com/Johnn3volta/simplest-calendar#readme",
  "devDependencies": {
    "@rollup/plugin-html": "^0.2.3",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-unused-imports": "^1.1.1",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.50.5",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.3.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "dependencies": {},
  "files": [
    "dist"
  ]
}
